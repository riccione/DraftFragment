<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/content_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <fragment
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:name="com.ignes.draftfragments.SettingsFragment$SettingsPreferenceFragment"
            android:id="@+id/preference_fragment"
            android:tag="@string/settings_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context="com.ignes.draftfragments.SettingsFragment"/>

</androidx.constraintlayout.widget.ConstraintLayout>